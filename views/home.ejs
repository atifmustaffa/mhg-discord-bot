<!DOCTYPE html>
<html lang="en">

<head>
    <!-- COMMON TAGS -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MHG Discord Bot</title>
    <meta name="description" content="An Awesome Discord Bot" />
    <link rel="icon" href="https://discord.com/assets/07dca80a102d4149e9736d4b162cff6f.ico">
    <!-- import the webpage's client-side css style file -->
    <link rel="stylesheet" href="style.css" />
    <!-- import the webpage's client-side javascript file -->
    <script src="client.js" defer></script>

    <!-- Google Fonts -->
    <link
        href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Roboto:100,300,400,500,700|Baloo+Chettan+2"
        rel="stylesheet">

    <!-- Bootstrap css -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">

    <!-- Libraries CSS Files -->
    <!-- <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
  <link href="lib/owlcarousel/assets/owl.theme.default.min.css" rel="stylesheet"> -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <!-- <link href="lib/animate/animate.min.css" rel="stylesheet"> -->
    <!-- <link href="lib/modal-video/css/modal-video.min.css" rel="stylesheet"> -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src='http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.5/jquery-ui.min.js'></script>
</head>

<body>

    <header id="header" class="header header-hide">
        <div class="container">

            <div id="logo" class="pull-left">
                <h1><a href="/">MHG</a></h1>
            </div>

            <nav id="nav-menu-container">
                <ul class="nav-menu">
                    <li class="menu-active"><a href="/">Home</a></li>
                    <li><a href="#commands">Commands</a></li>
                </ul>
            </nav><!-- #nav-menu-container -->
        </div>
    </header><!-- #header -->

    <!--==========================
    Hero Section
  ============================-->
    <section id="hero" class="wow fadeIn">
        <div class="hero-container">
            <h1>MHG</h1>
            <img src="https://cdn.discordapp.com/attachments/660818037292924938/714796390961643631/mhg-512x512-w.png"
                alt="mhg-images">
            <p class="status"><span class="icon-circle" style="vertical-align: middle;"></span><span
                    style="vertical-align: middle;">Online</span></p>
            <a href="#commands" class="btn-get-started">Commands</a>
            <a href="https://discord.com/api/oauth2/authorize?client_id=157730590492196864&scope=bot&permissions=8"
                class="btn-get-started">Add to server</a>
        </div>
    </section><!-- #hero -->

    <!--==========================
    Commands
  ============================-->

    <section id="commands" class="padd-section text-center wow fadeInUp"
        style="visibility: visible; animation-name: fadeInUp;">
        <div class="container">
            <div class="section-title text-center">
                <h2>Commands</h2>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md">
                    <div class="about-content">
                        <table class="table-commands">
                            <tr style="border-bottom: 1px solid #fff;">
                                <th><i class="fa fa-angle-down"></i></th>
                                <th>Command</th>
                                <th>Description</th>
                            </tr>
                            <%commands.commands.forEach(function(command) {%>
                                <tr>
                                    <td><i class="fa fa-angle-right"></i></td>
                                    <td>
                                        <%= config.prefix %>
                                            <%= command.name %>
                                    </td>
                                    <td>
                                        <%= command.value %>
                                    </td>
                                </tr>
                                <%});%>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--==========================
    Footer
  ============================-->
    <footer class="footer">

        <div class="copyrights">
            <div class="container">
                <p>Copyright &copy; 2020<br>Developed by <a href="https://www.twitter.com/atifmustaffa"
                        target="_blank">Atif Mustaffa</a></p>
            </div>
        </div>

    </footer>

</body>

</html>